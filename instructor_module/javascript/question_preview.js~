function mpcq_preview(){
   var q = $("#mpcq").val();
   $("#mpcq_preview").html(q);
   MathJax.Hub.Queue(["Typeset", MathJax.Hub]);
   return true;
}

function preview(src, target){
    var q = $(src).val();
    $(target).html(q);
    MathJax.Hub.Queue(["Typeset", MathJax.Hub]);
    return true;
}
